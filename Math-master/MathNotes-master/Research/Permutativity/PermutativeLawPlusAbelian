Permutatitive law with intmat a[2][2] = 1,1,0,1; \\

ring R=2,(c(1..2)(1..2)(1..2)),dp;
intmat a[2][2] = 0,1,1,0;
ideal I;
for(int i1 = 1; i1 <= 2; i1++)
{for(int i2 = 1; i2 <= 2; i2++)
{for(int i3 = 1; i3 <= 2; i3++)
{for(int i4 = 1; i4 <= 2; i4++)
{for(int k = 1; k <= 2; k++)
	{
        poly f(i1)(i2)(i3)(i4)(k);
	for (int j1 = 1; j1 <= 2; j1++)
        {for (int j2 = 1; j2 <= 2; j2++)
        {for (int j3 = 1; j3 <= 2; j3++)
        {for (int k1 = 1; k1 <= 2; k1++)
        {for (int k2 = 1; k2 <= 2; k2++)
                {
                f(i1)(i2)(i3)(i4)(k) = f(i1)(i2)(i3)(i4)(k) + 
                (a[i1,j1]*a[i2,j2]*c(i3)(i4)(k1)*c(k2)(j3)(k) - a[i3,j1]*a[i4,j2]*c(i1)(i2)(k1)*c(j3)(k2)(k))*a[k1,j3]*c(j1)(j2)(k2);
                }
                I= I+f(i1)(i2)(i3)(i4)(k);
        }}}}}
}}}};
I;




