Homassociative law with intmat a[2][2] = 1,1,0,1; \\


ring R=0,(c(1)(1)(1),c(1)(1)(2),c(1)(2)(1),c(1)(2)(2),c(2)(1)(1),c(2)(1)(2),c(2)(2)(1),c(2)(2)(2)),dp;
intmat a[2][2] = 1,1,0,1;
ideal I;
for(int i1 = 1; i1 <= 2; i1++)
{for(int i2 = 1; i2 <= 2; i2++)
{for(int i3 = 1; i3 <= 2; i3++)
{for(int k = 1; k <= 2; k++)
	{
        poly f(i1)(i2)(i3)(k);
	for (int j1 = 1; j1 <= 2; j1++)
        {for (int k1 = 1; k1 <= 2; k1++)
                {
                f(i1)(i2)(i3)(k) = f(i1)(i2)(i3)(k) + a[i1,j1]*c(i2)(i3)(k1)*c(j1)(k1)(k) - a[i3,j1]*c(i1)(i2)(k1)*c(k1)(j1)(k);
                }
                I= I+f(i1)(i2)(i3)(k);
        }}
}}};
std(I);

