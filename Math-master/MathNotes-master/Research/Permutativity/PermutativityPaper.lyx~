\begin_inset Text

\begin_layout Plain Layout
Permutative + Abelian
\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" topline="true" leftline="true" rightline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $0=\sum\limits _{j_{1},j_{2},j_{3},k_{1},k_{2}}(a_{i_{1}}^{j_{1}}a_{i_{2}}^{j_{2}}c_{i_{3},i_{4}}^{k_{1}}-a_{i_{3}}^{j_{1}}a_{i_{4}}^{j_{2}}c_{i_{1},i_{2}}^{k_{1}})a_{k_{1}}^{j_{3}}c_{j_{1},j_{2}}^{k_{2}}c_{k_{2},j_{3}}^{k}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
<row>
<cell alignment="center" valignment="top" topline="true" leftline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
Hom-Associative
\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" topline="true" leftline="true" rightline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $0=\sum\limits _{j_{1},k_{1}}(a_{i_{1}}^{j_{1}}c_{i_{2},i_{3}}^{k_{1}}c_{j_{1},k_{1}}^{k}-a_{i_{3}}^{j_{1}}c_{i_{1},i_{2}}^{k_{1}}c_{k_{1},j_{1}}^{k})$
\end_inset


\end_layout

\end_inset
</cell>
</row>
<row>
<cell alignment="center" valignment="top" topline="true" bottomline="true" leftline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
Hom-Associative + Abelian
\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" topline="true" bottomline="true" leftline="true" rightline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $0=\sum\limits _{j_{1},k_{1}}(a_{i_{1}}^{j_{1}}c_{i_{2},i_{3}}^{k_{1}}-a_{i_{3}}^{j_{1}}c_{i_{1},i_{2}}^{k_{1}})c_{j_{1},k_{1}}^{k}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
</lyxtabular>

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
0=\sum_{j_{1},k_{1}}(a_{i_{1}}^{j_{1}}c_{i_{2},i_{3}}^{k_{1}}c_{j_{1},k_{1}}^{k}-a_{i_{3}}^{j_{1}}c_{i_{1},i_{2}}^{k_{1}}c_{k_{1},j_{1}}^{k})
\]

\end_inset


\end_layout

\begin_layout Standard
If we assume that multiplication map is abelian, then we would have
\begin_inset Formula 
\[
\sum_{j_{1},j_{2},j_{3},k_{1},k_{2}}C_{j_{1}j_{2}}^{k_{2}}C_{j_{3}k_{2}}^{k_{3}}a_{j_{3}k_{1}}\left(C_{i_{1}i_{2}}^{k_{1}}a_{j_{1}i_{3}}a_{j_{2}i_{4}}-C_{i_{3}i_{4}}^{k_{1}}a_{j_{1}i_{1}}a_{j_{2}i_{2}}\right)=0
\]

\end_inset


\end_layout

\begin_layout Standard
If it is unital, then we would have 
\begin_inset Formula $C_{i1}^{k}=a_{ki}=C_{1i}^{k}$
\end_inset

 for all 
\begin_inset Formula $i,k$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sum_{j_{1},k_{1}}\left(a_{j_{1}i_{1}}C_{i_{2}i_{3}}^{k_{1}}C_{j_{1}k_{1}}^{k_{2}}-a_{j_{1}i_{3}}C_{i_{1}i_{2}}^{k_{1}}C_{k_{1}j_{1}}^{k_{2}}\right).
\]

\end_inset


\begin_inset Formula 
\[
\sum_{j_{1},k_{1}}\left(a_{j_{1}i_{1}}C_{i_{2}i_{3}}^{k_{1}}C_{j_{1}k_{1}}^{k_{2}}-a_{j_{1}i_{3}}C_{i_{1}i_{2}}^{k_{1}}C_{k_{1}j_{1}}^{k_{2}}\right).
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset

Now suppose 
\begin_inset Formula $\alpha$
\end_inset

 is an 
\begin_inset Formula $R$
\end_inset

-algebra homomorphism.
 Then
\begin_inset Formula 
\begin{align*}
\alpha(e_{i_{1}}e_{i_{2}}) & =\sum_{k_{1}}C_{i_{1}i_{2}}^{k_{1}}\alpha(e_{k_{1}})\\
 & =\sum_{k_{1},m}C_{i_{1}i_{2}}^{k_{1}}a_{mk_{1}}e_{m}
\end{align*}

\end_inset

and
\begin_inset Formula 
\begin{align*}
\alpha(e_{i_{1}})\alpha(e_{i_{2}}) & =\sum_{j_{1},j_{2}}a_{j_{1}i_{1}}a_{j_{2}i_{2}}e_{j_{1}}e_{j_{2}}\\
 & =\sum_{j_{1},j_{2},m}C_{j_{1}j_{2}}^{m}a_{j_{1}i_{1}}a_{j_{2}i_{2}}e_{m}
\end{align*}

\end_inset

Thus we need
\begin_inset Formula 
\[
\sum_{k_{1}}C_{i_{1}i_{2}}^{k_{1}}a_{mk_{1}}=\sum_{j_{1},j_{2}}C_{j_{1}j_{2}}^{m}a_{j_{1}i_{1}}a_{j_{2}i_{2}}
\]

\end_inset


\begin_inset Formula 
\[
\sum_{j_{1},j_{2},j_{3},k_{1},k_{2}}C_{j_{1}j_{2}}^{k_{2}}a_{j_{3}k_{1}}\left(C_{i_{1}i_{2}}^{k_{1}}C_{j_{3}k_{2}}^{k_{3}}a_{j_{1}i_{3}}a_{j_{2}i_{4}}-C_{i_{3}i_{4}}^{k_{1}}C_{k_{2}j_{3}}^{k_{3}}a_{j_{1}i_{1}}a_{j_{2}i_{2}}\right)
\]

\end_inset


\begin_inset Formula 
\[
\sum_{j_{1},k_{1}}\left(C_{i_{1}i_{2}}^{k_{1}}C_{k_{1}j_{1}}^{k_{2}}a_{j_{1}i_{3}}-C_{i_{2}i_{3}}^{k_{1}}C_{j_{1}k_{1}}^{k_{2}}a_{j_{1}i_{1}}\right)
\]

\end_inset


\begin_inset Formula 
\[
\sum_{j_{1},j_{2},j_{3},k_{1},k_{2}}C_{j_{1}j_{2}}^{k_{2}}C_{j_{3}k_{2}}^{k_{3}}a_{j_{3}k_{1}}\left(C_{i_{1}i_{2}}^{k_{1}}a_{j_{1}i_{3}}a_{j_{2}i_{4}}-C_{i_{3}i_{4}}^{k_{1}}a_{j_{1}i_{1}}a_{j_{2}i_{2}}\right)
\]

\end_inset


\begin_inset Formula 
\[
\sum_{j_{1},j_{2},j_{3},k_{1},k_{2}}C_{j_{3}k_{2}}^{k_{3}}a_{j_{3}k_{1}}\left(C_{j_{1}j_{2}}^{k_{2}}C_{i_{1}i_{2}}^{k_{1}}a_{j_{1}i_{3}}a_{j_{2}i_{4}}-C_{i_{3}i_{4}}^{k_{1}}C_{j_{1}j_{2}}^{k_{2}}a_{j_{1}i_{1}}a_{j_{2}i_{2}}\right)
\]

\end_inset


\begin_inset Formula 
\[
\sum_{j_{1},j_{2},j_{3},k_{1},k_{2}}C_{j_{3}k_{2}}^{k_{3}}\left(C_{j_{1}j_{2}}^{k_{2}}a_{j_{1}i_{3}}a_{j_{2}i_{4}}\left(C_{i_{1}i_{2}}^{k_{1}}a_{j_{3}k_{1}}\right)-a_{j_{3}k_{1}}C_{i_{3}i_{4}}^{k_{1}}\left(C_{j_{1}j_{2}}^{k_{2}}a_{j_{1}i_{1}}a_{j_{2}i_{2}}\right)\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Special Case
\end_layout

\begin_layout Standard
We consider a special case where 
\begin_inset Formula $n=2$
\end_inset

 and
\begin_inset Formula 
\[
[\alpha]_{\beta}=\begin{pmatrix}1 & 1\\
0 & 1
\end{pmatrix}.
\]

\end_inset

In this case, 
\begin_inset ERT
status open

\begin_layout Plain Layout

(
\backslash
ref{eq:permutative})
\end_layout

\end_inset

 simplifies as follows: for 
\begin_inset Formula $(i_{1},i_{2},i_{3},i_{4},k)=(1,1,1,1,1)$
\end_inset

, we have
\begin_inset Formula 
\[
\sum_{j_{1},j_{2},j_{3},k_{1},k_{2}}C_{j_{1}j_{2}}^{k_{2}}a_{j_{3}k_{1}}\left(C_{11}^{k_{1}}C_{j_{3}k_{2}}^{k_{3}}a_{j_{1}1}a_{j_{2}1}-C_{11}^{k_{1}}C_{k_{2}j_{3}}^{k_{3}}a_{j_{1}1}a_{j_{2}1}\right)
\]

\end_inset


\begin_inset Formula 
\[
\sum_{j_{1},j_{2},j_{3},k_{1},k_{2}}C_{j_{1}j_{2}}^{k_{2}}a_{j_{3}k_{1}}\left(C_{i_{1}i_{2}}^{k_{1}}C_{j_{3}k_{2}}^{k_{3}}a_{j_{1}i_{3}}a_{j_{2}i_{4}}-C_{i_{3}i_{4}}^{k_{1}}C_{k_{2}j_{3}}^{k_{3}}a_{j_{1}i_{1}}a_{j_{2}i_{2}}\right)
\]

\end_inset


\begin_inset Formula 
\[
\sum_{j_{1},j_{2},j_{3},k_{1},k_{2}}\left(C_{11}^{k_{1}}C_{j_{1}j_{2}}^{k_{2}}C_{j_{3}k_{2}}^{1}a_{j_{1}1}a_{j_{2}1}a_{j_{3}k_{1}}-C_{11}^{k_{1}}C_{j_{1}j_{2}}^{k_{2}}C_{k_{2}j_{3}}^{k}a_{j_{1}i_{1}}a_{j_{2}i_{2}}a_{j_{3}k_{1}}\right)=0
\]

\end_inset


\begin_inset Formula 
\[
\sum_{j_{1},j_{2},j_{3},k_{1},k_{2}}\left(C_{i_{1}i_{2}}^{k_{1}}C_{j_{1}j_{2}}^{k_{2}}C_{j_{3}k_{2}}^{k}a_{j_{1}i_{3}}a_{j_{2}i_{4}}a_{j_{3}k_{1}}-C_{i_{3}i_{4}}^{k_{1}}C_{j_{1}j_{2}}^{k_{2}}C_{k_{2}j_{3}}^{k}a_{j_{1}i_{1}}a_{j_{2}i_{2}}a_{j_{3}k_{1}}\right)=0
\]

\end_inset


\end_layout

\begin_layout Standard
Associativity:
\begin_inset Formula 
\begin{align*}
(e_{i_{1}}e_{i_{2}})e_{i_{3}} & =\sum_{j_{1}}C_{i_{1}i_{2}}^{j_{1}}e_{j_{1}}e_{i_{3}}\\
 & =\sum_{j_{1},j_{2}}C_{i_{1}i_{2}}^{j_{1}}C_{j_{1}i_{3}}^{j_{2}}e_{j_{2}}
\end{align*}

\end_inset

and
\begin_inset Formula 
\begin{align*}
e_{i_{1}}(e_{i_{2}}e_{i_{3}}) & =\sum C_{i_{2}i_{3}}^{j_{1}}e_{i_{1}}e_{j_{1}}\\
 & =\sum C_{i_{2}i_{3}}^{j_{1}}C_{i_{1}j_{1}}^{j_{2}}e_{j_{2}}
\end{align*}

\end_inset

so for each 
\begin_inset Formula $j_{2}$
\end_inset

, we need
\begin_inset Formula 
\[
\sum_{j_{1},j_{2}}C_{i_{1}i_{2}}^{j_{1}}C_{j_{1}i_{3}}^{j_{2}}=\sum_{j_{1},j_{2}}C_{i_{2}i_{3}}^{j_{1}}C_{i_{1}j_{1}}^{j_{2}}
\]

\end_inset


\end_layout

\begin_layout Subsection
Morphisms of Permutative 
\begin_inset Formula $R$
\end_inset

-Algebras
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{defn}
\backslash
label{defn}
\end_layout

\end_inset

 Let 
\begin_inset Formula $(A,\star,\alpha)$
\end_inset

 and 
\begin_inset Formula $(A',\star',\alpha')$
\end_inset

 be two permutative 
\begin_inset Formula $R$
\end_inset

-algebra.
 A map 
\begin_inset Formula $\varphi\colon A\to A'$
\end_inset

 is called a 
\series bold
morphism
\series default
 of permutative 
\begin_inset Formula $R$
\end_inset

-algebras if it satifies the following:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\varphi$
\end_inset

 is an 
\begin_inset Formula $R$
\end_inset

-module homomorphism of the underlying 
\begin_inset Formula $R$
\end_inset

-modules;
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\varphi$
\end_inset

 respects multiplications: 
\begin_inset Formula $\varphi(a\star b)=\varphi(a)\star'\varphi(b)$
\end_inset

 for all 
\begin_inset Formula $a,b\in A$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $\varphi$
\end_inset

 commutes with 
\begin_inset Formula $\alpha$
\end_inset

 and 
\begin_inset Formula $\alpha'$
\end_inset

: 
\begin_inset Formula $\varphi(\alpha(a))=\alpha'(\varphi(a))$
\end_inset

 for all 
\begin_inset Formula $a\in A$
\end_inset

.
 
\end_layout

\begin_layout Standard
We say 
\begin_inset Formula $\varphi$
\end_inset

 is an 
\series bold
isomorphism 
\series default
of permutative 
\begin_inset Formula $R$
\end_inset

-algebras if 
\begin_inset Formula $\varphi$
\end_inset

 is an isomorphism of the underlying 
\begin_inset Formula $R$
\end_inset

-modules.
 Similarly, we say 
\begin_inset Formula $A$
\end_inset

 and 
\begin_inset Formula $A'$
\end_inset

 are 
\series bold
isomorphic
\series default
 as permutative 
\begin_inset Formula $R$
\end_inset

-algebras if there exists an isomorphism between them.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{defn}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{prop}
\backslash
label{prop}
\end_layout

\end_inset

 Let 
\begin_inset Formula $(A,\star,\alpha)$
\end_inset

 be a permutative 
\begin_inset Formula $R$
\end_inset

-algebra and let 
\begin_inset Formula $\varphi\colon A\to A$
\end_inset

 be an 
\begin_inset Formula $R$
\end_inset

-module automorphism.
 Define 
\begin_inset Formula $R$
\end_inset

-linear maps 
\begin_inset Formula $\star_{\varphi}\colon A\otimes_{R}A\to A$
\end_inset

 and 
\begin_inset Formula $\alpha_{\varphi}\colon A\to A$
\end_inset

 by
\begin_inset Formula 
\[
a\star_{\varphi}b=\varphi(\varphi^{-1}(a)\star\varphi^{-1}(b))\quad\text{and}\quad\alpha_{\varphi}(a)=\varphi(\alpha(\varphi^{-1}(a)))
\]

\end_inset

for all 
\begin_inset Formula $a,b\in A$
\end_inset

.
 Then 
\begin_inset Formula $(A_{\varphi},\star_{\varphi},\alpha_{\varphi})$
\end_inset

 is a permutative 
\begin_inset Formula $R$
\end_inset

-algebra.
 Furthermore, 
\begin_inset Formula $A$
\end_inset

 is isomorphic to 
\begin_inset Formula $A_{\varphi}$
\end_inset

 as permutative 
\begin_inset Formula $R$
\end_inset

-algebras.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{prop}
\end_layout

\end_inset


\begin_inset Formula 
\begin{align*}
[\mathrm{d}(a)\mathrm{d}(b)]\mathrm{d}(cd) & =[\mathrm{d}(a)\mathrm{d}(b)][\mathrm{d}(c)d+c\mathrm{d}(d)]\\
 & =[\mathrm{d}(a)\mathrm{d}(b)][\mathrm{d}(c)d]+[\mathrm{d}(a)\mathrm{d}(b)][c\mathrm{d}(d)]\\
 & =\mathrm{d}(a)\mathrm{d}(b)\mathrm{d}(c)d+\mathrm{d}(a)\mathrm{d}(b)c\mathrm{d}(d)\\
\\
 & =\mathrm{d}(a)b\mathrm{d}(c)\mathrm{d}(d)+a\mathrm{d}(b)\mathrm{d}(c)\mathrm{d}(d)\\
 & =[\mathrm{d}(a)b][\mathrm{d}(c)\mathrm{d}(d)]+[a\mathrm{d}(b)][\mathrm{d}(c)\mathrm{d}(d)]\\
 & =[\mathrm{d}(a)b+a\mathrm{d}(b)][\mathrm{d}(c)\mathrm{d}(d)]\\
 & =\mathrm{d}(ab)[\mathrm{d}(c)\mathrm{d}(d)]
\end{align*}

\end_inset


\end_layout

\begin_layout Subsection
Example
\end_layout

\begin_layout Standard
Permutative 
\begin_inset Formula $R$
\end_inset

-algebra 
\begin_inset Formula $\mathbb{C}^{2}$
\end_inset

 with 
\begin_inset Formula 
\[
\alpha=\begin{pmatrix}1 & \lambda\\
0 & 1
\end{pmatrix}
\]

\end_inset

for 
\begin_inset Formula $\lambda\in\mathbb{C}$
\end_inset

.
 For 
\begin_inset Formula $(i_{1},i_{2},i_{3},i_{4},k)=(1,1,1,1,1)$
\end_inset

, we have
\end_layout

