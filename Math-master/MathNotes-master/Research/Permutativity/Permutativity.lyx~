\begin_layout Standard
\begin_inset Formula $1\star2=-\left(3\star2\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $2\star2=-\left(2\star2\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $1\star3=-\left(3\star1\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $2\star1=-\left(2\star3\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $1\star1=-\left(3\star3\right)$
\end_inset


\end_layout

\begin_layout Standard
Now assume 
\begin_inset Formula $(a\star b)\star c=-a\star(b\star-c)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $1\star3=(1\star0)\star3=3\star(0\star1)=3\star1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $2\star3=(2\star0)\star3=2\star(0\star1)=2\star1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $1\star2=(1\star0)\star2=3\star(0\star2)=3\star2$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $1\star1=(1\star0)\star1=3\star(0\star3)=3\star3$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $a\star b=(a\star0)\star b=f(a)\star(0\star f^{-1}(b))=f(a)\star f^{-1}(b)$
\end_inset

.
 So if 
\begin_inset Formula $f(a)=b$
\end_inset

, then 
\begin_inset Formula $a\star b=b\star a$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $(a\star b)\star c=f(a)\star(b\star f^{-1}(c))$
\end_inset

.
 So if 
\begin_inset Formula $f(a)=c$
\end_inset

, then 
\begin_inset Formula $(a\star b)\star c=c\star(b\star a)$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $a\star b=f^{-1}(f(a)\star f(b))$
\end_inset

.
 
\end_layout

\begin_layout Standard
\begin_inset Formula $a\star1=(1\star1)\star1=3\star(1\star3)=3\star c$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $b\star3=(1\star2)\star3=3\star(2\star1)=3\star d$
\end_inset


\end_layout

\begin_layout Standard
\align center
\begin_inset Tabular
<lyxtabular version="3" rows="5" columns="5">
<features tabularvalignment="middle">
<column alignment="center" valignment="top" width="0pt">
<column alignment="center" valignment="top" width="0pt">
<column alignment="center" valignment="top" width="0pt">
<column alignment="center" valignment="top" width="0pt">
<column alignment="center" valignment="top" width="0pt">
<row>
<cell alignment="center" valignment="top" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\star$
\end_inset


\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" bottomline="true" leftline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $0$
