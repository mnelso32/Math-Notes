<cell alignment="center" valignment="top" topline="true" leftline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $-a$
\end_inset


\end_layout

\end_inset
</cell>
</row>
</lyxtabular>

\end_inset


\end_layout

\begin_layout Standard
Now assume 
\begin_inset Formula $(a\star b)\star c=-a\star(b\star-c)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $1\star7=7\star1$
\end_inset


\end_layout

