\begin_layout Standard
We have
\begin_inset Formula 
\begin{align*}
\sum_{i,j,k}a_{i,j}^{0}c_{i,j}^{k}e_{k} & =\sum_{i,j}a_{i,j}^{0}e_{i}\star e_{j}\\
 & =\sum_{i,j}\sum_{i',j'}a_{i',j'}^{e_{i}}c_{i',j'}^{e_{j}}e_{i}\star e_{j}\\
 & =
\end{align*}

\end_inset

We have
\begin_inset Formula 
\begin{align*}
\sum_{i,j}g_{i,j}f_{i,j} & =\sum_{i,j}\sum_{\alpha}a_{i,j}^{\alpha}e_{\alpha}f_{i,j}\\
 & =\sum_{i,j}\left(\sum_{\alpha}a_{i,j}^{\alpha}e_{\alpha}e_{i}e_{j}+\sum_{\beta,k}a_{i,j}^{\beta}c_{i,j}^{k}e_{\beta}e_{k}\right)\\
\\
 & =\sum_{i,j}\left(\sum_{\alpha}a_{i,j}^{\alpha}e_{\alpha+i+j}+\sum_{\beta,k}a_{i,j}^{\beta}c_{i,j}^{k}e_{\beta+k}\right)\\
\\
 & =\sum_{i,j}\left(\sum_{\gamma}a_{i,j}^{\gamma-i-j}e_{\gamma}+\sum_{\beta,k}a_{i,j}^{\beta}c_{i,j}^{k}e_{\gamma}\right) &  & \alpha+i+j=\gamma=\beta+k\\
\\
 & =\sum_{i,j}\left(\sum_{\gamma}a_{i,j}^{\gamma-i-j}e_{\gamma}+\sum_{\gamma}\sum_{\beta,k}a_{i,j}^{\beta}c_{i,j}^{k}e_{\gamma}\right)\\
\\
 & =\sum_{i,j,\gamma}\left(a_{i,j}^{\gamma-i-j}+\sum_{k}a_{i,j}^{\gamma-k}c_{i,j}^{k}\right)e_{\gamma} &  & \alpha+i+j=\beta+k\\
\\
\end{align*}

\end_inset

We have 
\begin_inset Formula $\sum_{i,j}g_{i,j}f_{i,j}$
\end_inset

 
\begin_inset Formula 
\begin{align*}
\sum_{i,j}a_{i,j}^{0}e_{i}\star e_{j} & =\sum_{i,j}\sum_{k}a_{i,j}^{i+j-k}c_{i,j}^{k}e_{i}\star e_{j}\\
 & =
\end{align*}

\end_inset

For the coefficient of 
\begin_inset Formula $e_{i}e_{j}$
\end_inset

 in 
\begin_inset Formula $\sum_{i,j}g_{i,j}f_{i,j}$
\end_inset

 to be zero, we need 
\begin_inset Formula 
\[
a_{i,j}^{0}=\sum_{i_{1},j_{1}}a_{i_{1},j_{1}}^{e_{i}}c_{i_{1},j_{1}}^{e_{j}}
\]

\end_inset

where 
\begin_inset Formula $a_{i_{1},j_{1}}^{e_{i}}$
\end_inset

 is the coefficient of 
\begin_inset Formula $e_{i}$
\end_inset

 in 
\begin_inset Formula $g_{i_{1},j_{1}}$
\end_inset

 and 
\begin_inset Formula $c_{i_{1},j_{1}}^{e_{j}}$
\end_inset

 is the coefficient of 
\begin_inset Formula $e_{j}$
\end_inset

 in 
\begin_inset Formula $e_{i_{1}}\star e_{j_{1}}$
\end_inset

.
 For the coefficient of 
\begin_inset Formula $e_{i}e_{j}e_{k}$
\end_inset

 in 
\begin_inset Formula $\sum_{i,j}g_{i,j}f_{i,j}$
\end_inset

 to be zero, we need 
\begin_inset Formula 
\[
\sum_{k}a_{i,j}^{e_{k}}=\sum_{i_{1},j_{1}}a_{i_{1},j_{1}}^{e_{i}e_{j}}c_{i_{1},j_{1}}^{e_{k}}
\]

\end_inset

where 
\begin_inset Formula $a_{i_{1},j_{1}}^{e_{i}}$
\end_inset

 is the coefficient of 
\begin_inset Formula $e_{i}$
\end_inset

 in 
\begin_inset Formula $g_{i_{1},j_{1}}$
\end_inset

 and 
\begin_inset Formula $c_{i_{1},j_{1}}^{e_{j}}$
\end_inset

 is the coefficient of 
\begin_inset Formula $e_{j}$
\end_inset

 in 
\begin_inset Formula $e_{i_{1}}\star e_{j_{1}}$
\end_inset

.
 
\end_layout

\begin_layout Section
Example
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $R=K[x,y,z,w]$
\end_inset

, let 
\begin_inset Formula $I=\langle x^{2},w^{2},zw,xy,y^{2}z^{2}\rangle$
\end_inset

, and let 
\begin_inset Formula $F$
\end_inset

 be the free minimal resolution of 
\begin_inset Formula $R\slash I$
\end_inset

 over 
\begin_inset Formula $R$
\end_inset

.
 Consider the multiplication on 
\begin_inset Formula $F$
\end_inset

 given by
\end_layout

\begin_layout Standard
\align center
\begin_inset Tabular
<lyxtabular version="3" rows="6" columns="6">
