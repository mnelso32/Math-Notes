\begin_layout Subsection*
Another Calculation
\end_layout

\begin_layout Standard
We calculate
\begin_inset Formula 
\begin{align*}
[e_{i},e_{j},e_{k}] & =(e_{i}\star e_{j})\star e_{k}-e_{i}\star(e_{j}\star e_{k})\\
 & =\sum_{l}c_{i,j}^{l}e_{l}\star e_{k}-\sum_{l}c_{j,k}^{l}e_{i}\star e_{l}\\
 & =\sum_{m}\left(\sum_{l}(c_{i,j}^{l}c_{l,k}^{m}-c_{i,l}^{m}c_{j,k}^{l})\right)e_{m}
\end{align*}

\end_inset


\end_layout

\begin_layout Subsubsection
Yet Another
\end_layout

\begin_layout Standard
Suppose 
\begin_inset Formula 
\[
g_{1,1}f_{1,1}+g_{1,2}f_{1,2}+g_{2,2}f_{2,2}=b_{1}e_{1}+b_{2}e_{2}+b_{3}e_{3}
\]

\end_inset

We have
\begin_inset Formula 
\begin{align*}
b_{1}e_{1}+b_{2}e_{2}+b_{3}e_{3} & =g_{1,1}(e_{1}e_{1}+e_{1}\star e_{1})+g_{1,2}(e_{1}e_{2}+e_{1}\star e_{2})+g_{2,2}(e_{2}e_{2}+e_{2}\star e_{2})
\end{align*}

\end_inset

In degree 
\begin_inset Formula $1$
\end_inset

 we have 
\begin_inset Formula 
\[
b_{1}e_{1}+b_{2}e_{2}+b_{3}e_{3}=a_{1,1}^{(0,0)}e_{1}\star e_{1}+a_{1,2}^{(0,0)}e_{1}\star e_{2}+a_{2,2}^{(0,0)}e_{2}\star e_{2}
\]

\end_inset

In degree 
\begin_inset Formula $2$
\end_inset

 we have
\begin_inset Formula 
\[
0=(a_{1,1}^{(0,0)}+e_{1}e_{1}+a_{1,2}^{(0,0)}e_{1}e_{2}+a_{2,2}^{(0,0)}e_{2}e_{2}
\]

\end_inset


\begin_inset Formula 
\[
0=(a_{1,1}^{(0,0)}+a_{1,1}^{(1,0)}c_{1,1}^{(1,0)})e_{1}e_{1}+a_{1,2}^{(0,0)}e_{1}e_{2}+a_{2,2}^{(0,0)}e_{2}e_{2}
\]

\end_inset

