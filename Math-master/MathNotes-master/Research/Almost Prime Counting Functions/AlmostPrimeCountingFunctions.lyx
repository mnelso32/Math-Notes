\begin_layout Subsection
Twisting the Almost Prime Counting Functions by a Multiplicative Function
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{defn}
\backslash
label{defn}
\end_layout

\end_inset

 A function 
\begin_inset Formula $\varphi\colon\mathbb{N}\to\mathbb{C}$
\end_inset

 is said to be 
\series bold
completely multiplicative 
\series default
if 
\begin_inset Formula $\varphi(mn)=\varphi(m)\varphi(n)$
\end_inset

 for all 
\begin_inset Formula $m,n\in\mathbb{N}$
\end_inset

.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{defn}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $\varphi\colon\mathbb{N}\to\mathbb{C}$
\end_inset

 be a completely multiplicative function, let 
\begin_inset Formula $k\in\mathbb{N}$
\end_inset

, and let 
\begin_inset Formula $p$
\end_inset

 be a prime.
 We define the functions 
\begin_inset Formula $\pi_{k}^{\varphi}\colon\mathbb{R}_{\geq0}\to\mathbb{C}$
\end_inset

 and 
\begin_inset Formula $\pi_{k,p}^{\varphi}\colon\mathbb{R}_{\geq0}\to\mathbb{C}$
\end_inset

 by the formulas
\begin_inset Formula 
\[
\pi_{k}^{\varphi}(x)=\sum_{m\in S_{k}(x)}\varphi(m)\quad\text{and}\quad\pi_{k,p}^{\varphi}(x)=\sum_{m\in S_{k,p}(x)}\varphi(m)
\]

\end_inset

for all 
\begin_inset Formula $x\in\mathbb{R}_{\geq0}$
\end_inset

.
 Note that we recover the functions 
\begin_inset Formula $\pi_{k}$
\end_inset

 and 
\begin_inset Formula $\pi_{k,p}$
\end_inset

 by setting 
\begin_inset Formula $\varphi$
\end_inset

 to be the identity function.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{theorem}
\backslash
label{theoremrecursive}
\end_layout

\end_inset

 Let 
\begin_inset Formula $\varphi\colon\mathbb{N}\to\mathbb{C}$
\end_inset

 be a completely multiplicative function, let 
\begin_inset Formula $k,n\in\mathbb{N}$
\end_inset

 such that 
\begin_inset Formula $k\leq n$
\end_inset

, and let 
\begin_inset Formula $p$
\end_inset

 be a prime.
 Then 
\begin_inset Formula 
\[
\pi_{k}^{\varphi}(p^{n})=\varphi(p)\pi_{k-1}^{\varphi}(p^{n-1})+\pi_{k,p}^{\varphi}(p^{n})
\]

\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{theorem}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{proof}
\backslash
label{proof}
\end_layout

\end_inset

 Indeed, we have 
\begin_inset Formula 
\begin{align*}
\pi_{k}^{\varphi}(p^{n}) & =\sum_{m\in S_{k}(p^{n})}\varphi(m)\\
 & =\sum_{m\in S_{k-1}(p^{n-1})}\varphi(pm)+\sum_{m\in S_{k,p}(p^{n})}\varphi(m)\\
 & =\varphi(p)\sum_{m\in S_{k-1}(p^{n-1})}\varphi(m)+\sum_{m\in S_{k,p}(p^{n})}\varphi(m)\\
 & =\varphi(p)\pi_{k-1}^{\varphi}(p^{n-1})+\pi_{k,p}^{\varphi}(p^{n}).
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{proof}
\end_layout

\end_inset


\end_layout

\begin_layout Subsection
Number Fields
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $K$
\end_inset

 be a number field and let 
\begin_inset Formula $\mathcal{O}_{K}$
\end_inset

 be its ring of integers.
 Let 
\begin_inset Formula $\mathfrak{a}$
\end_inset

 be an ideal in 
\begin_inset Formula $\mathcal{O}_{K}$
\end_inset

 and suppose 
\begin_inset Formula $\mathfrak{a}=\mathfrak{p}_{1}^{e_{1}}\cdots\mathfrak{p}_{s}^{e_{s}}$
\end_inset

 is the prime factorization of 
\begin_inset Formula $\mathfrak{a}$
\end_inset

.
 The 
\series bold
degree of 
\begin_inset Formula $\mathfrak{a}$
\end_inset


\series default
, denoted 
\begin_inset Formula $\deg\mathfrak{a}$
\end_inset

, is given by
\begin_inset Formula 
\[
\deg\mathfrak{a}:=\sum_{r=1}^{s}f_{r}e_{r},
\]

\end_inset

where 
\begin_inset Formula $f_{r}=[\mathcal{O}_{K}/\mathfrak{p}_{r}:\mathbb{Z}/(\mathfrak{p}_{r}\cap\mathbb{Z})]$
\end_inset

 for each 
\begin_inset Formula $r=1,\dots,s$
\end_inset

.
 In particular, if 
\begin_inset Formula $\mathfrak{p}$
\end_inset

 is a prime ideal, then the degree of 
\begin_inset Formula $\mathfrak{p}$
\end_inset

 is the degree of the field extension 
\begin_inset Formula $\mathbb{Z}/p\hookrightarrow\mathcal{O}_{K}/\mathfrak{p}$
\end_inset

 where 
\begin_inset Formula $p=\mathfrak{p}\cap\mathbb{Z}$
\end_inset

.
 This explains why we chose our terminology in the way that we did.
 To be complete, we also require 
\begin_inset Formula $\text{deg}\langle1\rangle=0$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset

For each prime ideal 
\begin_inset Formula $\mathfrak{p}$
\end_inset

, we also define 
\begin_inset Formula 
\[
\text{ord}_{\mathfrak{p}}(\mathfrak{a})=\begin{cases}
e_{r} & \text{if }\mathfrak{p}=\mathfrak{p}_{r}\text{ for some }r=1,\dots,s\\
0 & \text{otherwise}.
\end{cases}
\]

\end_inset

The 
\series bold
norm 
\series default
of 
\begin_inset Formula $\mathfrak{a}$
\end_inset

 is given by 
\begin_inset Formula $\text{N}(\mathfrak{a}):=\#\mathcal{O}_{K}/\mathfrak{a}$
\end_inset

.
 Note that the norm is a multiplicative function, and so in particular we
 have
\begin_inset Formula 
\begin{align*}
\text{N}(\mathfrak{a}) & =\#\mathcal{O}_{K}/\mathfrak{a}\\
 & =\left(\#\mathcal{O}_{K}/\mathfrak{p}_{1}^{e_{1}}\right)\cdots\left(\#\mathcal{O}_{K}/\mathfrak{p}_{s}^{e_{s}}\right)\\
 & =p_{1}^{f_{1}e_{1}}\cdots p_{s}^{f_{s}e_{s}},
\end{align*}

\end_inset

where 
\begin_inset Formula $p_{r}$
\end_inset

 denotes the prime 
\begin_inset Formula $\mathfrak{p}_{r}\cap\mathbb{Z}$
\end_inset

 for each 
\begin_inset Formula $r=1,\dots,s$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset

For each 
\begin_inset Formula $x\in\mathbb{R}_{\geq0}$
\end_inset

, 
\begin_inset Formula $k\in\mathbb{N}$
\end_inset

, and 
\begin_inset Formula $\mathfrak{p}$
\end_inset

 prime ideal in 
\begin_inset Formula $\mathcal{O}_{K}$
\end_inset

, we define the sets 
\begin_inset Formula 
\[
S_{k}(x):=\left\{ \mathfrak{a}\in\text{Ideal}(\mathcal{O}_{K})\mid\text{N}(\mathfrak{a})\leq x\text{ and }\deg\mathfrak{a}=k\right\} \quad\text{and}\quad S_{k,\mathfrak{p}}(x):=\left\{ \mathfrak{a}\in\text{Ideal}(\mathcal{O}_{K})\mid\text{N}(\mathfrak{a})\leq x,\text{ }\deg\mathfrak{a}=k\text{, and }\text{ord}_{\mathfrak{p}}(\mathfrak{a})=0\right\} .
\]

\end_inset

Similarly, for each 
\begin_inset Formula $k\in\mathbb{N}$
\end_inset

 and 
\begin_inset Formula $\mathfrak{p}$
\end_inset

 prime, we define the functions 
\begin_inset Formula $\pi_{k}\colon\mathbb{R}_{\geq0}\to\mathbb{N}$
\end_inset

 and 
\begin_inset Formula $\pi_{k,\mathfrak{p}}\colon\mathbb{R}_{\geq0}\to\mathbb{N}$
\end_inset

 by 
\begin_inset Formula 
\[
\pi_{k}(x)=|S_{k}(x)|\quad\text{and}\quad\pi_{k,\mathfrak{p}}(x)=|S_{k,\mathfrak{p}}(x)|
\]

\end_inset

for all 
\begin_inset Formula $x\in\mathbb{R}_{\geq0}$
\end_inset

.
\end_layout

