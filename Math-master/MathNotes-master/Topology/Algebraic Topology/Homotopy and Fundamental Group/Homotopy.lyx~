xmin=-4,xmax=4,
\end_layout

\begin_layout Plain Layout

ymin=-4,ymax=4,  
\end_layout

\begin_layout Plain Layout

axis equal,
\end_layout

\begin_layout Plain Layout

]
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
addplot [color=red,samples=80,domain=0:6.28] ({cos(2*deg(x)) + cos(deg(x))
 + 2},{sin(2*deg(x)) + sin(deg(x))}); 
\end_layout

