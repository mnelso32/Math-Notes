\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{lemma}
\backslash
label{lemmakoszulcomplex}
\end_layout

\end_inset

 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
hfill
\end_layout

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $K(x_{1},\dots,x_{n})_{\bullet}$
\end_inset

 is a complex, that is 
\begin_inset Formula $d_{p}\circ d_{p+1}=0$
\end_inset

 for all 
\begin_inset Formula $p$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
Let 
\begin_inset Formula $H_{i}(x_{1},\dots,x_{n},M)$
\end_inset

 be the homology of 
\begin_inset Formula $K(x_{1},\dots,x_{n},M)_{\bullet}$
\end_inset

, that is 
\begin_inset Formula 
\[
H_{i}(x_{1},\dots,x_{n},M):=\mbox{Ker}(d_{i}\otimes1_{M})/\mbox{Im}(d_{i+1}\otimes1_{M}),
\]

\end_inset

 then 
\begin_inset Formula $H_{0}(x_{1},\dots,x_{n},M)=M/\langle x_{1},\dots,x_{n}\rangle M$
\end_inset

 and 
\begin_inset Formula $H_{n}(x_{1},\dots,x_{n},M)=\{m\in M\mid x_{i}m=0\mbox{ for all }i\}\cong\mbox{Hom}_{A}(A/\langle x_{1},\dots,x_{n}\rangle,M)=\text{Soc}(M)$
\end_inset

.
 
\end_layout

\begin_layout Enumerate
\begin_inset Formula $K(x_{1},\dots,x_{n},M)_{\bullet}=K(x_{1},M)_{\bullet}\otimes\cdots\otimes K(x_{n},M)_{\bullet}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{lemma}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{proof}
\end_layout

\end_inset

 The first two statements are straightforward.
 To prove the third statement, recall the definition of tensor product of
 complexes: Let 
\begin_inset Formula $(L_{\bullet},d_{\bullet})$
\end_inset

 and 
\begin_inset Formula $(K_{\bullet},d_{\bullet})$
\end_inset

 be two complexes of 
\begin_inset Formula $A$
\end_inset

-modules.
 Then the tensor product of 
\begin_inset Formula $(L_{\bullet},d_{\bullet})$
\end_inset

 and 
\begin_inset Formula $(K_{\bullet},d_{\bullet})$
\end_inset

 is 
\begin_inset Formula $(L_{\bullet}\otimes K_{\bullet},d_{\bullet})$
\end_inset

, where 
\begin_inset Formula 
\[
(L_{\bullet}\otimes K_{\bullet})_{n}=\bigoplus_{p+q=n}L_{p}\otimes K_{q},
\]

\end_inset


\end_layout

\begin_layout Standard
and for 
\begin_inset Formula $x\in L_{p}$
\end_inset

 and 
\begin_inset Formula $y\in K_{q}$
\end_inset

 we have 
\begin_inset Formula 
\[
d_{n}(x\otimes y)=d_{p}(x)\otimes y+(-1)^{p}x\otimes d_{q}(y).
\]

\end_inset


\end_layout

\begin_layout Standard
Using induction, it is enough to prove that 
\begin_inset Formula 
\[
K(x_{1},\dots,x_{n})_{\bullet}=K(x_{1},\dots,x_{n-1})_{\bullet}\otimes K(x_{n})_{\bullet}.
\]

\end_inset


\end_layout

\begin_layout Standard
Observe that 
\begin_inset Formula $(K(x_{1},\dots,x_{n-1})_{\bullet}\otimes K(x_{n})_{\bullet})_{p}\cong K(x_{1},\dots,x_{n-1})_{p}\oplus K(x_{1},\dots,x_{n-1})_{p-1}$
\end_inset

 is free with generators given by 
\begin_inset Formula $e_{i_{1},\dots,i_{p}}\otimes1$
\end_inset

 and 
\begin_inset Formula $e_{j_{1},\dots,j_{p-1}}\otimes e_{n}$
\end_inset

 where 
\begin_inset Formula $1\leq i_{1}<\cdots<i_{p}\leq n-1$
\end_inset

 and 
\begin_inset Formula $1\leq j_{1}<\cdots<j_{p-1}\leq n-1$
\end_inset

.
 Define a map 
\begin_inset Formula $\varphi_{p}:(K(x_{1},\dots,x_{n-1})_{\bullet}\otimes K(x_{n})_{\bullet})_{p}\to(K(x_{1},\dots,x_{n})_{\bullet})_{p}$
\end_inset

 by sending generators of the form 
\begin_inset Formula $e_{i_{1},\dots,i_{p}}\otimes1$
\end_inset

 to 
\begin_inset Formula $e_{i_{1},\dots,i_{p}}$
\end_inset

, and sending generators of the form 
\begin_inset Formula $e_{j_{1},\dots,j_{p-1}}\otimes e_{n}$
\end_inset

 to 
\begin_inset Formula $e_{j_{1},\dots,j_{p-1},n}$
\end_inset

, and then extend this map linearly.
 The map 
\begin_inset Formula $\varphi_{p}$
\end_inset

 is clearly an isomorphism, we just need to be sure it commutes with the
 differential.
 We have 
\begin_inset Formula 
\begin{align*}
d_{p}(e_{i_{1},\dots,i_{p}}\otimes1) & =d_{p}(e_{i_{1},\dots,i_{p}})\otimes1+(-1)^{p}e_{i_{1},\dots,i_{p}}\otimes d_{0}(1)\\
 & =\left(\sum_{\mu=1}^{p}(-1)^{\mu-1}x_{i_{\mu}}e_{i_{1}\dots i_{\mu-1}i_{\mu+1}\dots i_{p}}\right)\otimes1\\
 & =\sum_{\mu=1}^{p}(-1)^{\mu-1}x_{i_{\mu}}(e_{i_{1}\dots i_{\mu-1}i_{\mu+1}\dots i_{p}}\otimes1),
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
and 
\begin_inset Formula $\varphi_{p-1}(d_{p}(e_{i_{1},\dots,i_{p}}\otimes1))=\sum_{\mu=1}^{p}(-1)^{\mu-1}x_{i_{\mu}}e_{i_{1}\dots i_{\mu-1}i_{\mu+1}\dots i_{p}}.$
\end_inset

 Next we have 
\begin_inset Formula 
\begin{align*}
d_{p}(e_{j_{1},\dots,j_{p-1}}\otimes e_{n}) & =d_{p-1}(e_{j_{1},\dots,j_{p-1}})\otimes e_{n}+(-1)^{p-1}e_{j_{1},\dots,j_{p-1}}\otimes x_{n}\\
 & =\sum_{\mu=1}^{p-1}(-1)^{\mu-1}x_{i_{\mu}}e_{j_{1}\dots j_{\mu-1}j_{\mu+1}\dots j_{p-1}}\otimes e_{n}+(-1)^{p-1}x_{n}e_{j_{1},\dots,j_{p-1}}\otimes1,
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
and 
\begin_inset Formula $\varphi_{p-1}(d_{p}(e_{j_{1},\dots,j_{p-1}}\otimes e_{n}))=\sum_{\mu=1}^{p-1}(-1)^{\mu-1}x_{i_{\mu}}e_{j_{1}\dots j_{\mu-1}j_{\mu+1}\dots j_{p-1}}+(-1)^{p-1}x_{n}e_{j_{1},\dots,j_{p-1}}.$
\end_inset


\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{proof}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{cor}
\end_layout

\end_inset

 Let 
\begin_inset Formula $A$
\end_inset

 be a ring, 
\begin_inset Formula $M$
\end_inset

 an 
\begin_inset Formula $A$
\end_inset

-module and 
\begin_inset Formula $x_{1},\dots,x_{n}\in A$
\end_inset

.
 Let 
\begin_inset Formula $x=(x_{1},\dots,x_{n})$
\end_inset

 and 
\begin_inset Formula $y=(x_{\pi(1)},\dots,x_{\pi(n)})$
\end_inset

 for a permutation 
\begin_inset Formula $\pi$
\end_inset

.
 Then the complexes 
\begin_inset Formula $K(x,M)_{\bullet}$
\end_inset

 and 
\begin_inset Formula $K(y,M)_{\bullet}$
\end_inset

 are isomorphic.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{cor}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


