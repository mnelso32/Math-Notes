\backslash
begin{pmatrix} XY & XZ & YZ  
\backslash
end{pmatrix} } 
\backslash
& 
\backslash
& 
\backslash
& 
\backslash
& 
\backslash
& R 
\backslash
arrow{r} 
\backslash
& R/I 
\backslash
end{tikzcd}
\backslash
end{center}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Then for 
\begin_inset Formula $i=2$
\end_inset

, we have
\begin_inset Formula 
\begin{align*}
I_{r_{2}}(\partial_{2})=I_{2}\begin{pmatrix}-Z & -Z\\
Y & 0\\
0 & X
\end{pmatrix} & =\langle XY,XZ,YZ\rangle,
\end{align*}

\end_inset

and 
\begin_inset Formula $\text{depth}_{I_{r_{2}}(\partial_{2})}(R)=2$
\end_inset

.
 For 
\begin_inset Formula $i=1$
\end_inset

, we have
\begin_inset Formula 
\begin{align*}
I_{r_{1}}(\partial_{1})=I_{1}(XY,YZ,YZ) & =\langle XY,XZ,YZ\rangle,
\end{align*}

\end_inset

and 
\begin_inset Formula $\text{depth}_{I_{r_{2}}(\partial_{2})}(R)=\text{depth}_{I}(R)=2$
\end_inset

.
 
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{example}
\end_layout

\end_inset


