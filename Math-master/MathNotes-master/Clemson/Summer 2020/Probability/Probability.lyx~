\begin_layout Subsubsection*
More Properties
\end_layout

\begin_layout Itemize
if 
\begin_inset Formula $X\sim\mathrm{Poisson}(\theta)$
\end_inset

, 
\begin_inset Formula $Y\sim\mathrm{Poisson}(\lambda)$
\end_inset

, and 
\begin_inset Formula $X$
\end_inset

 and 
\begin_inset Formula $Y$
\end_inset

 are independent, then 
\begin_inset Formula $X+Y\sim\mathrm{Poisson}(\theta+\lambda)$
\end_inset

.
 
\end_layout

