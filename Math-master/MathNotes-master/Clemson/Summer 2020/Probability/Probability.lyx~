\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $M_{X}(t)=\frac{1}{1-\beta t}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
</lyxtabular>

\end_inset


\end_layout

