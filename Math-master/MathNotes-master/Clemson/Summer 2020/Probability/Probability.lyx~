\begin_layout Subsubsection*
More Properties
\end_layout

\begin_layout Itemize
if 
\begin_inset Formula $X\sim\mathrm{beta}(\alpha,\beta)$
\end_inset

, 
\begin_inset Formula $Y\sim\mathrm{beta}(\alpha+\beta,\gamma)$
\end_inset

, and 
\begin_inset Formula $X$
\end_inset

 and 
\begin_inset Formula $Y$
\end_inset

 are independent, then 
\begin_inset Formula $XY\sim\mathrm{beta}(\alpha,\beta+\gamma)$
\end_inset

.
 
\end_layout

\begin_layout Standard
\begin_inset Newpage newpage
\end_inset


\end_layout

