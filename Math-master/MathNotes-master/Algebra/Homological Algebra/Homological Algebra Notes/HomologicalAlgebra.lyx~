\backslash
otimes _R B ) 
\backslash
arrow[d, 
\begin_inset Quotes eld
\end_inset

 
\backslash
mathrm{H} ( P_2 
\backslash
otimes _R  
\backslash
psi  )   
\begin_inset Quotes eld
\end_inset

] 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash

\backslash

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
mathrm{H} ( P_1 
\backslash
otimes _R B' ) 
\backslash
arrow[r, 
\begin_inset Quotes eld
\end_inset


\backslash
