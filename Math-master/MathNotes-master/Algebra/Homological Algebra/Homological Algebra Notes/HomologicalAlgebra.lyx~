\backslash
{  2 
\backslash
} } - x_2 e_{ 
\backslash
{  1 
\backslash
} }
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash

\backslash
 
\backslash
& 
\backslash
& e_{ 
\backslash
{  1 
\backslash
} } 
\backslash
arrow[r,mapsto,shorten >=0.2cm,shorten <=0.2cm] 
\backslash
& x_1 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash

\backslash
 
\backslash
& 
\backslash
& e_{ 
\backslash
{  2 
\backslash
} } 
\backslash
arrow[r,mapsto,shorten >=0.2cm,shorten <=0.2cm] 
\backslash
& x_2 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash

\backslash
 
\backslash
& 
\backslash
& e_{ 
\backslash
{  3 
\backslash
} } 
\backslash
arrow[r,mapsto,shorten >=0.2cm,shorten <=0.2cm] 
\backslash
& x_3
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
end{tikzcd}
\backslash
end{center}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
end{example}
