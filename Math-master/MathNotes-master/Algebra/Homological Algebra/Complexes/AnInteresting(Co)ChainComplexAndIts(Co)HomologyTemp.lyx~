\end_inset


\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" topline="true" leftline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $d:(S_{I})_{i}\to(S_{I})_{i-1}$
\end_inset


\end_layout

\end_inset
</cell>
</row>
