\backslash
arrow[r," d_0 "] & A_{-1}  
\backslash
arrow[r," d_{-2} d _{-1} "] & A_{-3} 
\backslash
arrow[r, "d_{-3}"] & A_{-4} 
\backslash
arrow[r] & 
\backslash
cdots.
 
\backslash
end{tikzcd}
\backslash
