\end_layout

\end_inset


\end_layout

\begin_layout Standard
We 
\series bold
collapse
\series default
 
\begin_inset Formula $A$
\end_inset

 into a chain complex 
\begin_inset Formula $A_{\star}$
\end_inset

 as follows:
\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
begin{center}
\backslash
begin{tikzcd} A_{
\backslash
star } := 
\backslash
cdots 
\backslash
arrow[r] A_{5} 
\backslash
