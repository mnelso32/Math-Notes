\backslash

\backslash
 & H_{i-j-1} (S_{I:g} ) 
\backslash
arrow[r, "
\backslash
cdot g "] & H_{i-1} (S_I ) 
\backslash
arrow[r, "-^{G'} "] & 
\backslash
cdots 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash
