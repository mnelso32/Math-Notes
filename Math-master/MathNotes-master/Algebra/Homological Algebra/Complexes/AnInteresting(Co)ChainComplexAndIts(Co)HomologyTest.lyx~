\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash

\backslash
  & H_{i-j} (S_{I:g} ) 
\backslash
arrow[r, "
\backslash
cdot g"] & H_{i} (S_I ) 
\backslash
arrow[r, "-^{G'} "] 
\backslash
arrow[d, phantom, ""{coordinate, name=Z}] & H_{i} (S _{
\backslash
langle I 
\backslash
text{,} g 
\backslash
rangle }) 
\backslash
arrow[dll, swap, rounded corners, to path={ -- ([xshift=2ex]
\backslash
tikztostart.east) |- (Z) [near end]
\backslash
tikztonodes -| ([xshift=-2ex]
\backslash
