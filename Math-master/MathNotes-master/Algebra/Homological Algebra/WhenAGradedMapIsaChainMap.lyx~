\begin_layout Plain Layout


\backslash
begin{example}
\backslash
label{example}
\end_layout

\end_inset

 Let 
\begin_inset Formula $R=K[x,y,z]$
\end_inset

, let 
\begin_inset Formula $\underline{f}=f_{1},f_{2},f_{3}$
\end_inset

 be a sequence of elements in 
\begin_inset Formula $R$
\end_inset

, and let 
\begin_inset Formula $\pi\colon\mathcal{K}(\underline{f})\to\mathcal{K}(\underline{f})$
\end_inset

 be a graded 
\begin_inset Formula $R$
\end_inset

-linear map.
 For each 
\begin_inset Formula $1\leq i<j\leq3$
\end_inset

, we have
\begin_inset Formula 
\begin{align*}
\pi(1) & =f_{0}^{0}\\
\pi(e_{i}) & =f_{i}^{1}e_{1}+f_{i}^{2}e_{2}+f_{i}^{3}e_{3}\\
\pi(e_{ij}) & =f_{ij}^{12}e_{12}+f_{ij}^{13}e_{13}+f_{ij}^{23}e_{23}\\
\pi(e_{ijk}) & =f_{123}^{123}e_{123}
\end{align*}

\end_inset

where the 
\begin_inset Formula $f_{i}^{k}$
\end_inset

's and 
\begin_inset Formula $f_{ij}^{kl}$
\end_inset

's are in 
\begin_inset Formula $R$
\end_inset

.
 Then we have
\begin_inset Formula 
\begin{align*}
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(1) & =f_{0}^{0}\\
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(e_{i}) & =f_{i}^{1}f_{1}+f_{i}^{2}f_{2}+f_{i}^{3}f_{3}-f_{0}^{0}f_{i}\\
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(e_{ij}) & =(f_{j}f_{i}^{1}-f_{i}f_{j}^{1}-f_{ij}^{13}f_{3}-f_{ij}^{12}f_{2})e_{1}+(f_{j}f_{i}^{2}-f_{i}f_{j}^{2}-f_{ij}^{23}f_{3}+f_{ij}^{12}f_{1})e_{2}+(f_{j}f_{i}^{3}-f_{i}f_{j}^{3}+f_{ij}^{23}f_{j}+f_{ij}^{13})e_{3}
\end{align*}

\end_inset

One can calculate more generally that
\begin_inset Formula 
\begin{align*}
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(e_{1}) & =f_{1}^{1}f_{1}+f_{1}^{2}f_{2}+f_{1}^{3}f_{3}-f_{1}f_{0}
\end{align*}

\end_inset

We first calculate
\begin_inset Formula 
\begin{align*}
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(e_{1}) & =\mathrm{d}_{\mathcal{K}(\underline{f})}\pi(e_{1})-\pi\mathrm{d}_{\mathcal{K}(\underline{f})}(e_{1})\\
 & =\mathrm{d}_{\mathcal{K}(\underline{f})}(f_{1}^{1}e_{1}+f_{1}^{2}e_{2}+f_{1}^{3}e_{3})-\pi(f_{1})\\
 & =f_{1}^{1}f_{1}+f_{1}^{2}f_{2}+f_{1}^{3}f_{3}-f_{1}f_{0}
\end{align*}

\end_inset

More generally we have
\begin_inset Formula 
\[
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(e_{i})=f_{i}^{1}f_{1}+f_{i}^{2}f_{2}+f_{i}^{3}f_{3}-f_{i}f_{0}
\]

\end_inset

for 
\begin_inset Formula $i=1,2,3$
\end_inset

.
 
\end_layout

\begin_layout Standard
\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset


\begin_inset space ~
\end_inset

Next we calculate
\begin_inset Formula 
\begin{align*}
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(e_{12}) & =\mathrm{d}_{\mathcal{K}(\underline{f})}\pi(e_{12})-\pi\mathrm{d}_{\mathcal{K}(\underline{f})}(e_{12})\\
 & =\mathrm{d}_{\mathcal{K}(\underline{f})}(f_{12}^{12}e_{12}+f_{12}^{13}e_{13}+f_{12}^{23}e_{23})-\pi(f_{1}e_{2}-f_{2}e_{1})\\
 & =f_{12}^{12}(f_{1}e_{2}-f_{2}e_{1})+f_{12}^{13}(f_{1}e_{3}-f_{3}e_{1})+f_{12}^{23}(f_{2}e_{3}-f_{3}e_{2})-f_{1}(f_{2}^{1}e_{1}+f_{2}^{2}e_{2}+f_{2}^{3}e_{3})+f_{2}(f_{1}^{1}e_{1}+f_{1}^{2}e_{2}+f_{1}^{3}e_{3})\\
 & =(f_{2}f_{1}^{1}-f_{1}f_{2}^{1}-f_{12}^{13}f_{3}-f_{12}^{12}f_{2})e_{1}+(f_{2}f_{1}^{2}-f_{1}f_{2}^{2}-f_{12}^{23}f_{3}+f_{12}^{12}f_{1})e_{2}+(f_{2}f_{1}^{3}-f_{1}f_{2}^{3}+f_{12}^{23}f_{2}+f_{12}^{13})e_{3}
\end{align*}

\end_inset

Next we calculate
\begin_inset Formula 
\begin{align*}
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(e_{13}) & =\mathrm{d}_{\mathcal{K}(\underline{f})}\pi(e_{13})-\pi\mathrm{d}_{\mathcal{K}(\underline{f})}(e_{13})\\
 & =\mathrm{d}_{\mathcal{K}(\underline{f})}(e_{23})-\pi(f_{1}e_{3}-f_{3}e_{1})\\
 & =f_{2}e_{3}-f_{3}e_{2}-f_{1}e_{3}+f_{3}e_{2}\\
 & =(f_{2}-f_{1})e_{3}.
\end{align*}

\end_inset

Next we calculate
\begin_inset Formula 
\begin{align*}
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(e_{23}) & =\mathrm{d}_{\mathcal{K}(\underline{f})}\pi(e_{23})-\pi\mathrm{d}_{\mathcal{K}(\underline{f})}(e_{23})\\
 & =\mathrm{d}_{\mathcal{K}(\underline{f})}(e_{13})-\pi(f_{2}e_{3}-f_{3}e_{2})\\
 & =f_{1}e_{3}-f_{3}e_{1}-f_{2}e_{3}+f_{3}e_{1}\\
 & =(f_{1}-f_{2})e_{3}.
\end{align*}

\end_inset

Finally we calculate
\begin_inset Formula 
\begin{align*}
(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})(e_{123}) & =\mathrm{d}_{\mathcal{K}(\underline{f})}\pi(e_{123})-\pi\mathrm{d}_{\mathcal{K}(\underline{f})}(e_{123})\\
 & =\mathrm{d}_{\mathcal{K}(\underline{f})}(-e_{123})-\pi(f_{1}e_{23}-f_{2}e_{13}+f_{3}e_{12})\\
 & =-f_{1}e_{23}+f_{2}e_{13}-f_{3}e_{12}-f_{1}e_{13}+f_{2}e_{23}+f_{3}e_{12}\\
 & =(f_{2}-f_{1})(e_{23}+e_{13}).
\end{align*}

\end_inset

Thus, we have.
 
\begin_inset Formula 
\[
\mathrm{im}(\mathrm{d}_{\mathcal{K}(\underline{f})}\pi-\pi\mathrm{d}_{\mathcal{K}(\underline{f})})=\langle f_{2}-f_{1}\rangle
\]

\end_inset

 
