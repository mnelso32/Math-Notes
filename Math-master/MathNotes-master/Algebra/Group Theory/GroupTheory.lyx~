\backslash
arrow[uu,dash] & 
\backslash
mathbb{Q} ( r_2 ) 
\backslash
arrow[uul,dash] & 
\backslash
mathbb{Q} ( r_1 ) 
\backslash
arrow[uull,dash] 
\backslash

\backslash
 & 
\backslash
mathbb{Q}  
\backslash
arrow[u,dash] 
\backslash
arrow[ur, dash] 
\backslash
arrow[urr,dash] 
\backslash
arrow[urrr,dash] 
\backslash
arrow[urrrr,dash] 
\backslash
end{tikzcd}
\backslash
