\backslash
begin{center}
\backslash
begin{tikzcd}  & B_Q (R) 
\backslash
arrow[dr, "
\backslash
text{Ev} _1 "] 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash

\backslash
 
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

S/I 
\backslash
