\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout


\backslash

\backslash
  & H_{i-j} (S /( I:g )) 
\backslash
arrow[r, "
\backslash
cdot g"] & H_{i} (S/ I) 
\backslash
