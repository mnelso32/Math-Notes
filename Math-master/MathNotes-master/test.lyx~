\begin_layout Standard
h.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\frac{\mathrm{d}}{\mathrm{d}x}\left(3\sqrt{x^{2}+1}\right) & =3\frac{\mathrm{d}}{\mathrm{d}x}\left(\sqrt{x^{2}+1}\right)\\
 & =3\frac{\mathrm{d}}{\mathrm{d}x}\left((x^{2}+1)^{\frac{1}{2}}\right)\\
 & =3\cdot\frac{1}{2}(x^{2}+1)^{\frac{1}{2}-1}\cdot\frac{\mathrm{d}}{\mathrm{d}x}(x^{2}+1)\\
 & =3\cdot\frac{1}{2}(x^{2}+1)^{-\frac{1}{2}}\cdot\left(\frac{\mathrm{d}}{\mathrm{d}x}(x^{2})+\frac{\mathrm{d}}{\mathrm{d}x}(1)\right)\\
 & =3\cdot\frac{1}{2}(x^{2}+1)^{-\frac{1}{2}}\cdot\left(2x+0\right)\\
 & =3\cdot\frac{1}{2}(x^{2}+1)^{-\frac{1}{2}}\cdot2x.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
g.
 
\begin_inset Formula 
\begin{align*}
\frac{\mathrm{d}}{\mathrm{d}x}\left(2^{\ln(x)}\right) & =\ln(2)\cdot2^{\ln(x)}\cdot\frac{\mathrm{d}}{\mathrm{d}x}(\ln(x))\\
 & =\ln(2)\cdot2^{\ln(x)}\cdot\frac{1}{x}.
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
e.
\begin_inset Formula 
\begin{align*}
\frac{\mathrm{d}}{\mathrm{d}x}\left(\frac{5}{7(x^{3}-x)^{2}}\right) & =\frac{5}{7}\frac{\mathrm{d}}{\mathrm{d}x}\left(\frac{1}{(x^{3}-x)^{2}}\right)\\
 & =\frac{5}{7}\frac{\mathrm{d}}{\mathrm{d}x}\left((x^{3}-x)^{-2}\right)\\
 & =\frac{5}{7}\cdot-2(x^{3}-x)^{-3}\cdot\frac{\mathrm{d}}{\mathrm{d}x}(x^{3}-x)\\
 & =\frac{5}{7}\cdot-2(x^{3}-x)^{-3}\cdot(3x^{2}-1)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
b.
 
\begin_inset Formula 
\begin{align*}
\frac{\mathrm{d}}{\mathrm{d}t}\left(2e^{(3t^{2})}\right) & =2\frac{\mathrm{d}}{\mathrm{d}t}\left(e^{(3t^{2})}\right)\\
 & =2e^{(3t^{2})}\cdot\frac{\mathrm{d}}{\mathrm{d}t}(3t^{2})\\
 & =2e^{(3t^{2})}\cdot6t.
\end{align*}

\end_inset


\end_layout

